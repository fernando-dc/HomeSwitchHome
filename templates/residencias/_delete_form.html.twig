<form method="post" action="{{ path('residencias_delete', {'idResidencia': residencia.idResidencia}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ residencia.idResidencia) }}">
    <button class="btn">Delete</button>
</form>
